//Find the:--- Factor of a given number


#include<bits/stdc++.h>
using namespace std;


void factors(int num)
{

   for(int i=1; i <= num; i++)
   {
      if (num % i == 0)
      cout << i << " ";
   }
   
}


int main()
{
   int num;
   cout<<"Enter the number : "<<endl;
   cin>>num;
   
   cout << "\nThe factors of " << num << " are : ";
   factors(num);
   
   return 0;
}
