//   Check whether entered number is prime or not

#include<iostream>
using namespace std;

int main()
{
	int i,n;
	
	cout<<"Enter any positive number : "<<endl;
	cin>>n;
	
	
	for(i=2;i<n;i++)
	{
		if(n%i==0)
		{
			cout<<"The Entered number is not a prime number "<<n<<endl;
			break;
		}
	}
	if(n==i)
	{
		cout<<"The entered number is prime : "<<n<<endl;
	}
}
